{
    "version": 2,
    "builds": [
      {
        "src": "myproject/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3" }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "myproject/wsgi.py"
      }
    ],
    "outputDirectory": "staticfiles"
  }
